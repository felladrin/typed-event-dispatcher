{
  "name": "typed-event-dispatcher",
  "version": "1.10.1",
  "description": "Strongly-typed events that can be publicly listened but internally-only dispatched.",
  "type": "commonjs",
  "main": "dist/typed-event-dispatcher.js",
  "types": "dist/typed-event-dispatcher.d.ts",
  "files": [
    "dist",
    "readme-too.md"
  ],
  "scripts": {
    "build": "tsc --emitDeclarationOnly && parcel build src/typed-event-dispatcher.ts --no-source-maps --global typed-event-dispatcher",
    "test": "nve latest,lts jest --collectCoverage",
    "lint": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,json,md,yml}\" --ignore-path \".eslintignore\"",
    "docs": "typedoc --out docs --excludeNotExported",
    "prepare": "npm run build",
    "preview-package-contents": "npm pack --dry-run"
  },
  "keywords": [
    "event",
    "dispatcher",
    "signal",
    "emitter",
    "strongly",
    "typed",
    "typescript"
  ],
  "homepage": "https://www.victornogueira.app/typed-event-dispatcher/",
  "repository": "github:felladrin/typed-event-dispatcher",
  "author": {
    "name": "Victor Nogueira",
    "email": "victor@victornogueira.app",
    "url": "https://victornogueira.app"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^7.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.1",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "nve": "^13.0.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "ts-jest": "^26.0.0",
    "typedoc": "^0.17.7",
    "typescript": "^3.9.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint"
    }
  }
}
