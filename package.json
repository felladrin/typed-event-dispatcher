{
  "name": "typed-event-dispatcher",
  "version": "1.8.0",
  "description": "Strongly-typed events that can be publicly listened but internally-only dispatched.",
  "type": "commonjs",
  "main": "typed-event-dispatcher.js",
  "types": "typed-event-dispatcher.d.ts",
  "files": [
    "typed-event-dispatcher.js",
    "typed-event-dispatcher.d.ts"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest --collectCoverage",
    "lint": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,json,md,yml}\" --ignore-path \".eslintignore\"",
    "docs": "typedoc --out docs --excludeNotExported",
    "prepare": "npm run build",
    "preview-package-contents": "npm run prepare && npm pack --dry-run"
  },
  "keywords": [
    "event",
    "dispatcher",
    "signal",
    "emitter",
    "strongly",
    "typed",
    "typescript"
  ],
  "homepage": "https://www.victornogueira.app/typed-event-dispatcher/",
  "repository": "github:felladrin/typed-event-dispatcher",
  "author": {
    "name": "Victor Nogueira",
    "email": "victor@victornogueira.app",
    "url": "https://victornogueira.app"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "prettier": "^2.0.4",
    "pretty-quick": "^2.0.1",
    "ts-jest": "^25.3.1",
    "typedoc": "^0.17.4",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
